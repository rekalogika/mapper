{
    "name": "rekalogika/mapper",
    "description": "An object mapper for PHP and Symfony. Maps an object to another object. Primarily used for transforming an entity to a DTO and vice versa.",
    "homepage": "https://rekalogika.dev/mapper",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "mapper",
        "automapper",
        "dto",
        "transformation",
        "mapping",
        "api",
        "symfony",
        "api-platform",
        "lazy-loading",
        "proxy"
    ],
    "authors": [
        {
            "name": "Priyadi Iman Nurcahyo",
            "email": "priyadi@rekalogika.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "doctrine/collections": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.25",
        "psr/container": "^2.0",
        "symfony/cache": "^6.4 || ^7.0",
        "symfony/clock": "^6.4 || ^7.0",
        "symfony/config": "^6.4 || ^7.0",
        "symfony/console": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4.11 || ^6.5 || ^7.1.4 || ^7.2",
        "symfony/filesystem": "^6.4 || ^7.0",
        "symfony/finder": "^6.4 || ^7.0",
        "symfony/property-access": "^6.4 || ^7.0",
        "symfony/property-info": "6.4.0 - 6.4.8 || ^6.4.10 || ^6.5 || 7.0.0 - 7.0.8 || 7.1.0 - 7.1.1 || ^7.1.3 || ^7.2",
        "symfony/serializer": "^6.4 || ^7.0",
        "symfony/service-contracts": "^3.0",
        "symfony/var-exporter": "^6.4.1 || ^6.5 || ^7.0.1 || ^7.1"
    },
    "require-dev": {
        "bnf/phpstan-psr-container": "^1.0",
        "brick/money": "^0.10.0",
        "dave-liddament/php-language-extensions": "^0.9.0 ",
        "dave-liddament/phpstan-php-language-extensions": "^0.9.2",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^2.14 || ^3.0",
        "doctrine/persistence": "^3.1",
        "ekino/phpstan-banned-code": "^3.0",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^11.5",
        "psalm/plugin-phpunit": "^0.19.0",
        "psr/log": "^3.0",
        "ramsey/uuid": "^3.0 || ^4.0",
        "rector/rector": "^2.0",
        "symfony/debug-bundle": "^6.4 || ^7.0",
        "symfony/dotenv": "^6.4 || ^7.0",
        "symfony/framework-bundle": "^6.4 || ^7.0",
        "symfony/http-kernel": "^6.4 || ^7.0",
        "symfony/monolog-bundle": "^2.0 || ^3.0",
        "symfony/polyfill-php83": "^1.30",
        "symfony/runtime": "^6.4 || ^7.0",
        "symfony/stopwatch": "^6.4 || ^7.0",
        "symfony/uid": "^6.4 || ^7.0",
        "symfony/var-dumper": "^6.4 || ^7.0",
        "symfony/web-profiler-bundle": "^6.4 || ^7.0",
        "symfony/yaml": "^6.4 || ^7.0",
        "twig/twig": "^2.12|^3.0",
        "vimeo/psalm": "^6.0"
    },
    "suggest": {
        "phpstan/phpstan": "Required to automatically dump mapping metadata to a file (use --dev option to install)",
        "phpstan/extension-installer": "Configures PHPStan automatically (use --dev option to install)"
    },
    "autoload": {
        "psr-4": {
            "Rekalogika\\Mapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rekalogika\\Mapper\\Tests\\": "tests/src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "phpstan-extension.neon"
            ]
        },
        "branch-alias": {
            "dev-main": "1.10.x-dev"
        }
    }
}
